journal = {Ecology Letters},
keywords = {Algorithms,Bayes Theorem,Bayesian statistics,Computer Simulation,Ecosystem,Indirect inference,Intractable likelihood,Inverse modelling,Likelihood approximation,Likelihood-free inference,Markov Chains,Maximum likelihood,Model selection,Models,Monte Carlo Method,Nonparametric,Parameter estimation,Statistical,Statistics,Stochastic simulation},
