doi = {10.1002/eco.129},
file = {:home/jeroen/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bertoldi et al. - 2010 - Topographical and ecohydrological controls on land surface temperature in an alpine catchment.pdf:pdf},
issn = {1936-0584},
