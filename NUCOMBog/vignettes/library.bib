journal = {Journal of Vegetation Science},
keywords = {Carbon cycling,Competition,Ecosystem model,Global change,NUCOM-BOG,Ombrotrophic peatland,Palaeoecology,Species effect,Sphagnum,Vegetation composition},
month = {jun},
