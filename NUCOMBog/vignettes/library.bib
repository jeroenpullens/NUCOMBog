doi = {10.1073/pnas.1416267112},
file = {:home/jeroen/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Petrescu et al. - 2015 - The uncertain climate footprint of wetlands under human pressure.pdf:pdf},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
month = {apr},
number = {15},
pages = {4594--9},
pmid = {25831506},
title = {{The uncertain climate footprint of wetlands under human pressure.}},
