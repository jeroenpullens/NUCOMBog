number = {2},
pages = {283--291},
title = {{Measurements of Carbon Dioxide Fluxes by Chamber Method at the Rzecin Wetland Ecosystem, Poland}},
url = {http://connection.ebscohost.com/c/articles/50738359/measurements-carbon-dioxide-fluxes-by-chamber-method-rzecin-wetland-ecosystem-poland},
