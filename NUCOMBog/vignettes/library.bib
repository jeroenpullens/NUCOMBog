issn = {0921-2973},
journal = {Landscape Ecology},
keywords = {BALANCE,COSTA-RICA,CREATED RIVERINE MARSHES,Carbon dioxide,Carbon sequestration,GREENHOUSE GASES,Global carbon budget,METHANE EMISSIONS,Marsh,Methane,Methanogenesis,NUTRIENT ACCUMULATION,PEATLANDS,Peatland,SELF-DESIGN,SEQUESTRATION,Swamp,TROPICAL WETLANDS},
month = {jun},
