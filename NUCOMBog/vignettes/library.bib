doi = {10.5194/bg-10-929-2013},
file = {:home/jeroen/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Charman et al. - 2013 - Climate-related changes in peatland carbon accumulation during the last millennium.pdf:pdf},
