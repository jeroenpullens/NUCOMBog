journal = {Ecological Modelling},
keywords = {BOREAL PEATLANDS,CARBON-DIOXIDE EXCHANGE,CH4 flux,CO2 flux,Climate change,FEN ECOSYSTEM,Finland,METHANE EMISSIONS,Mire types,NORTHERN WETLANDS,OMBROTROPHIC BOG,SCOTS PINE,SPATIAL VARIATION,SUB-ARCTIC PEATLAND,WATER-TABLE},
