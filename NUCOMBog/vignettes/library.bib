issn = {0048-9697},
journal = {The Science of the total environment},
keywords = {Carbon,Carbon: metabolism,Ecosystem,Greenhouse Effect,Hydrogen-Ion Concentration,Photosynthesis,Plants,Seasons,Soil,Water Supply},
month = {aug},
number = {1-3},
pages = {133--46},
pmid = {12873406},
title = {{Carbon budget for a British upland peat catchment.}},
