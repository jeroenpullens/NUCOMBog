issn = {01681923},
journal = {Agricultural and Forest Meteorology},
keywords = {19,194,2014,8,ACCUMULATION,CARBON-DIOXIDE EXCHANGE,Carbon exchange,DENSITY,Ecosystem sensitivity,Eddy-Covariance,FLUX MEASUREMENTS,IRELAND,Ireland,NET ECOSYSTEM EXCHANGE,NORTHERN PEATLAND,Ombrotrophic peatland,RESPIRATION,SHORT-TERM,TEMPERATURE,agrformet,agricultural and forest meteorology,com,contents lists available at,e,elsevier,icultural and forest meteorology,j o ur na,l ho me pag,locate,sciencedirect,www},
