issn = {1354-1013},
journal = {Global Change Biology},
keywords = {2009,and accepted 10 may,carbon,ch 4,co 2,fire,greenhouse gas emissions,n 2 o,peat soil,received 6 march 2009,south-east asia,subsidence,tropical peatswamp},
