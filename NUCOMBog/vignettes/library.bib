file = {:home/jeroen/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Gnatowski et al. - 2010 - Hydraulic properties of fen peat soils in Poland.pdf:pdf},
issn = {00167061},
journal = {Geoderma},
keywords = {188,195,2010,CONDUCTIVITY,Cluster analysis,DATABASE,INVERSE METHOD,MODEL,MULTISTEP OUTFLOW,Moisture retention characteristics,PARAMETER-ESTIMATION,Peat soil,RETENTION,STEP OUTFLOW EXPERIMENTS,Unsaturated hydraulic conductivity,WATER PRESSURE DATA,contents lists available at,derma 154,sciencedirect},
month = {jan},
