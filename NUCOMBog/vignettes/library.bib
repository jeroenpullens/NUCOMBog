title = {{Responses of mosses Sphagnum capillifolium and Polytrichum strictum to nitrogen deposition in a bog: height growth, ground cover, and CO2 exchange}},
url = {http://www.nrcresearchpress.com/doi/abs/10.1139/cjb-2015-0183?src=recsys{\&}{\#}.VlLnHnarRaR},
year = {2015}
