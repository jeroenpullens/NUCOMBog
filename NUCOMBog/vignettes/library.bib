@article{Gauci2004,
abstract = {Natural wetlands form the largest source of methane (CH(4)) to the atmosphere. Emission of this powerful greenhouse gas from wetlands is known to depend on climate, with increasing temperature and rainfall both expected to increase methane emissions. This study, combining our field and controlled environment manipulation studies in Europe and North America, reveals an additional control: an emergent pattern of increasing suppression of methane (CH(4)) emission from peatlands with increasing sulfate (SO(4)(2-)-S) deposition, within the range of global acid deposition. We apply a model of this relationship to demonstrate the potential effect of changes in global sulfate deposition from 1960 to 2080 on both northern peatland and global wetland CH(4) emissions. We estimate that sulfur pollution may currently counteract climate-induced growth in the wetland source, reducing CH(4) emissions by approximately 15 Tg or 8{\%} smaller than it would be in the absence of global acid deposition. Our findings suggest that by 2030 sulfur pollution may be sufficient to reduce CH(4) emissions by 26 Tg or 15{\%} of the total wetland source, a proportion as large as other components of the CH(4) budget that have until now received far greater attention. We conclude that documented increases in atmospheric CH(4) concentration since the late 19th century are likely due to factors other than the global warming of wetlands.},
author = {Gauci, Vincent and Matthews, Elaine and Dise, Nancy and Walter, Bernadette and Koch, Dorothy and Granberg, Gunnar and Vile, Melanie A.},
doi = {10.1073/pnas.0404412101},
file = {:home/jeroen/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Gauci et al. - 2004 - Sulfur pollution suppression of the wetland methane source in the 20th and 21st centuries.pdf:pdf},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Acid Rain,Air Pollutants,Atmosphere,Climate,Earth (Planet),Ecosystem,Environmental Monitoring,Europe,Forecasting,Greenhouse Effect,Methane,Methane: metabolism,Models, Theoretical,North America,Sulfur,Sulfur: metabolism},
month = {aug},
number = {34},
pages = {12583--7},
pmid = {15297612},
title = {{Sulfur pollution suppression of the wetland methane source in the 20th and 21st centuries.}},
url = {http://www.pnas.org/content/101/34/12583.full},
volume = {101},
year = {2004}
}
@article{Gavazov2015,
abstract = {Peatlands are important sinks of atmospheric carbon (C) that, in response to climate warming, are undergoing dynamic vegetation succession. Here we examined the hypothesis that the uptake of nutrients by different plant growth forms (PGFs) is one key mechanism driving changes in species abundance in peatlands. Along an altitude gradient representing a natural climate experiment, we compared the variability of the stable C isotope composition ($\delta$(13)C) and stable nitrogen (N) isotope composition ($\delta$(15)N) in current-year leaves of two major PGFs, i.e. ericoids and graminoids. The climate gradient was associated with a gradient of vascular plant cover, which was parallelled by different concentrations of organic and inorganic N as well as the fungal/bacterial ratio in peat. In both PGFs the (13)C natural abundance showed a marginal spatial decrease with altitude and a temporal decrease with progression of the growing season. Our data highlight a primary physical control of foliar $\delta$(13)C signature, which is independent from the PGFs. Natural abundance of foliar (15)N did not show any seasonal pattern and only in the ericoids showed depletion at lower elevation. This decreasing $\delta$(15)N pattern was primarily controlled by the higher relative availability of organic versus inorganic N and, only for the ericoids, by an increased proportion of fungi to bacteria in soil. Our space-for-time approach demonstrates that a change in abundance of PGFs is associated with a different strategy of nutrient acquisition (i.e. transfer via mycorrhizal symbiosis versus direct fine-root uptake), which could likely promote observed and predicted dwarf shrub expansion under climate change.},
author = {Gavazov, Konstantin and Hagedorn, Frank and Buttler, Alexandre and Siegwolf, Rolf and Bragazza, Luca},
doi = {10.1007/s00442-015-3458-4},
file = {:home/jeroen/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Gavazov et al. - 2015 - Environmental drivers of carbon and nitrogen isotopic signatures in peatland vascular plants along an altitude g.pdf:pdf},
issn = {0029-8549},
journal = {Oecologia},
month = {oct},
pmid = {26433961},
title = {{Environmental drivers of carbon and nitrogen isotopic signatures in peatland vascular plants along an altitude gradient}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26433961 http://link.springer.com/10.1007/s00442-015-3458-4},
year = {2015}
}
