journal = {Nature},
keywords = {Arctic Regions,Biomass,Carbon,Carbon: analysis,Carbon: metabolism,Cold Climate,Ecosystem,Fertilizers,Greenhouse Effect,Nitrogen,Nitrogen: analysis,Nitrogen: metabolism,Plants,Plants: metabolism,Soil,Soil: analysis,Time Factors},
