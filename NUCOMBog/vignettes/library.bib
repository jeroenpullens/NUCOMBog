journal = {Agricultural and Forest Meteorology},
keywords = {CO2 fluxes,Closed chamber,Eddy-covariance,Footprint,Spatial variability,Wind direction},
month = {aug},
number = {1-4},
pages = {231--243},
title = {{Estimating net ecosystem exchange in a patterned ecosystem: Example from blanket bog}},
