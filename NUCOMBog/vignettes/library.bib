issn = {0167-6369},
journal = {Environmental Monitoring and Assessment},
month = {sep},
number = {9},
pages = {550},
pmid = {26239569},
title = {{Annual ecosystem respiration variability of alpine peatland on the eastern Qinghai–Tibet Plateau and its controlling factors}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26239569 http://link.springer.com/10.1007/s10661-015-4733-x},
