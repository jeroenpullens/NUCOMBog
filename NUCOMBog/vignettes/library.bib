file = {:home/jeroen/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Tfaily et al. - 2013 - Investigating dissolved organic matter decomposition in northern peatlands using complimentary analytical techniq.pdf:pdf},
issn = {00167037},
journal = {Geochimica et Cosmochimica Acta},
keywords = {BOG,CARBON,FLUORESCENCE,FULVIC-ACIDS,ION-CYCLOTRON-RESONANCE,LAKE AGASSIZ PEATLAND,MASS-SPECTROMETRY,METHANE PRODUCTION,MINNESOTA,PORE-WATER},
